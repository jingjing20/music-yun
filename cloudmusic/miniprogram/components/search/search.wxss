@import 'iconfont.wxss';
.container {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  position: relative;
}

.jing {
  flex: 1;
  height: 72rpx;
  font-size: 28rpx;
  border-radius: 50px;
  background-color: #f5f5f5;
  padding-left: 55rpx;
}

.search {
  background: #d43c33;
  color: #fff;
  margin-left: 10rpx;
  font-size: 15px !important;
  border-radius: 10px;
}

.in-bar {
  color: #999;
}

.find {
  position: absolute;
  top: 50%;
  transform: translate(16rpx,-50%);
  color: #9f9f9f;
}